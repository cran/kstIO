@comment{baumunk:empg96,
author ={Katja Baumunk and Cornelia E. Dowling and Cord Hockemeyer},
title ={Detecting Inconsistent and Invalid Items from Experts'
Knowledge Structures and Students' Data},
year = 1997,
note = subm,
abstract ={In the framework of knowledge space theory, prerequisite
relationships between learning objectives from a knowledge domain are
obtained from experts' judgments. The validity of these prerequisite
relationships is tested by comparing them to students' test data.
Thereby, the experts' judgments can differ from each other, and the
students' data can contradict the experts' prerequisite relationships.
We suggest coefficients for detecting the inconsistencies and
invalidities, and evaluate these coefficients in two simulation
studies. The performance of two of the coefficients can be shown to be
efficient, whereas the results for the validity coefficients are not
yet promising.}
}

